		public void FromGrid(int index)
		{
			if (index == 0)
			{
				HelperFunctions.FromGrid(GetHeaderRow().POID,
					m_emDataSet.POItemTbl,poGrid);
			}
			else
			{
				DataTable sourceTable = m_acknowledgementGrid.GetTable();
				EMDataSet.POItemTblDataTable targetTable = m_emDataSet.POItemTbl;
				foreach (DataRow sourceRow in sourceTable.Rows)
				{
					int poItemNumber = (int)sourceRow["POItemNumber"];
					EMDataSet.POItemTblRow targetRow = 
						targetTable.FindByPOItemNumber(poItemNumber);
					foreach (DataColumn column in sourceTable.Columns)
					{
						string columnName = column.ColumnName;
						targetRow[columnName] = sourceRow[columnName];
					}
				}
			}
			FixUMButNoUpdates(umCombo.Text);
		}

		public override void FromControls()
		{
			// Depending on the selected tab, we commit changes from the particular grids
			FromSelectedGrid();

			EMDataSet.POHeaderTblRow row = GetHeaderRow();
			for (int i=0;i<m_textBoxes.Length;i++)
			{
				try
				{
					row[m_fieldNames[i]] = GetText(m_textBoxes[i]);
				}
				catch (Exception e)
				{
					throw new Exception("Could not parse field:" + m_fieldNames[i] + "\n" +
						e.Message);
				}
			}
			try
			{
				if (m_exchangeRateEdt.Text.Length == 0)
					row["ExchangeRate"] = 1;
				else
					row["ExchangeRate"] = m_exchangeRateEdt.Text;
			}
			catch( Exception e)
			{
				throw new Exception("Could not parse field: Exchange Rate" + e.Message);
			}

			for (int i=0;i<m_dateBoxes.Length;i++)
			{
				if  (m_dateBoxes[i].Text == "")
					row[m_dateFieldNames[i]] = DBNull.Value;
				else
					row[m_dateFieldNames[i]] = System.DateTime.Parse(m_dateBoxes[i].Text);
			}
			row.VendName = millCombo.Text;
			row.ShipToName = customerCombo.Text;

			row.VendLocationName = vendLocationCombo.Text;
			row.ShipToLocationName = shipToLocationCombo.Text;

			// First update the total:
			decimal total = 0;
			foreach (EMDataSet.POItemTblRow detailRow in GetAllDetailRows())
			{
				if (!detailRow.IsCustAmountNull())
					total += detailRow.CustAmount;
			}
			row.OtherTotal = total;
			decimal exchange;
			if (row.IsExchangeRateNull())
				exchange = 1;
			else
				exchange = row.ExchangeRate;
			row.USTotal = total * exchange;
		}
		public static void CompareForValidaty(int poid,EMDataSet unsavedDataSet,EMDataSet currentDataSet)
		{
			EMDataSet.POHeaderTblRow unsavedRow = unsavedDataSet.POHeaderTbl.FindByPOID(poid);
			EMDataSet.POHeaderTblRow currentRow = currentDataSet.POHeaderTbl.FindByPOID(poid);
			if (currentRow == null)
				return; // This is OK. 
			if (unsavedRow == null)
				throw new Exception("Logic error. There is no current purchase order");
			DataRowCollection unsavedChildrenTemp = unsavedDataSet.POItemTbl.Rows;
			ArrayList listOfChildren = new ArrayList();
			foreach (EMDataSet.POItemTblRow row in unsavedChildrenTemp)
			{
				if (row.RowState != DataRowState.Added)
					listOfChildren.Add(row);
			}
			EMDataSet.POItemTblRow[] currentChildren = (EMDataSet.POItemTblRow[])
				currentRow.GetChildRows("POHeaderTblPOItemTbl");
			if (currentChildren.Length != listOfChildren.Count)
			{
				throw new Exception("Number of purchase order items changed since the last refresh. Update aborted");
			}
			foreach (EMDataSet.POItemTblRow currentChild in currentChildren)
			{
				int itemNumber = currentChild.POItemNumber;
				EMDataSet.POItemTblRow unsavedChild = null;
				foreach (EMDataSet.POItemTblRow temp in unsavedDataSet.POItemTbl)
				{
					if ((int)temp["POItemNumber",DataRowVersion.Original] == itemNumber)
					{
						unsavedChild = temp;
						break;
					}
				}
				if (unsavedChild == null)
					throw new Exception("A purchase order item has been added since the last refresh. Update aborted");
				short originalSeqNumber = (short)unsavedChild["SeqNumber",DataRowVersion.Original];
						if (originalSeqNumber != currentChild.SeqNumber)
							throw new Exception("At least one of the purchase order items changed position " + 
								"since the last refresh. Update aborted");		
				foreach (DataColumn column in unsavedChild.Table.Columns)
				{
					object original = unsavedChild[column,DataRowVersion.Original];
					object current = currentChild[column];
					if ((original.GetType() != current.GetType()) ||
						(!original.Equals(current)))
					{
						string message = "Unable to save. At least some of the data in the list " + 
									"of purchase order items is out of date with the latest in the " + 
									"database. You must cancel your change and re-save. This is a " +
									"result of another user making a change while you were editing " + 
									"this purchase order.\n";
						
						string cause = "Cause\n";
						if (!currentChild.IsItemNameNull())
							cause = "Item Name:" + currentChild.ItemName + "\n";
						cause += "Field Name:" + column.ColumnName + "\n";
						cause += "Database Value:";
						if (!currentChild.IsNull(column.ColumnName))
							cause += currentChild[column.ColumnName].ToString();
						cause += "\n";
						cause += "Your original Value:";
						if (!unsavedChild.IsNull(column,DataRowVersion.Original))
						{
							cause += unsavedChild[column,DataRowVersion.Original].ToString();
						}
						throw new Exception(message + cause);
					}
				}

			}
		}
		public override void CommitTablesToDatabase()
		{
			using (FileStream str = DataInterface.CreateLockFile("po.lock"))
			{
				try 
				{
					emConnection.Open();
					EMDataSet tempDataSet = new EMDataSet();
					DataInterface.FillAdapterWithParameter(poHeaderAdapter,base.CurrentKey);
					poHeaderAdapter.Fill(tempDataSet.POHeaderTbl);
					DataInterface.FillAdapterWithParameter(poItemAdapter,base.CurrentKey);
					poItemAdapter.Fill(tempDataSet.POItemTbl);
					if (!IsEmptyTable())
					{
						EMDataSet.POHeaderTblRow row = GetHeaderRow();
						if (DataInterface.IsRowAlive(row))
							CompareForValidaty(row.POID,m_emDataSet,tempDataSet);
					}

					DataInterface.UpdateTableAdd(poHeaderAdapter,m_emDataSet.POHeaderTbl);
					DataInterface.UpdateTableDelete(poItemAdapter,m_emDataSet.POItemTbl);
					DataInterface.UpdateTableDelete(poHeaderAdapter,m_emDataSet.POHeaderTbl);
					DataInterface.UpdateTableAdd(poItemAdapter,m_emDataSet.POItemTbl);					
				}
				finally
				{
					emConnection.Close();
				}
			}
		}

		void UpdateCurrencyControls()
		{
			EMDataSet.POHeaderTblRow row = GetHeaderRow();
            if (row.IsExchangeRateNull())
				m_exchangeRateEdt.Text = "";
			else
				m_exchangeRateEdt.Text = row.ExchangeRate.ToString();

			if (row.IsOtherTotalNull())
				m_totalCostEdit.Text = "";
			else
				m_totalCostEdit.Text = row.OtherTotal.ToString("F2");
			if (row.IsUSTotalNull())
				m_totalCostUSEdit.Text = "";
			else
				m_totalCostUSEdit.Text = row.USTotal.ToString("F2");
		}

		public override void UpdateControls() 
		{
			using (new StopComboBoxUpdates(this))
			{
				UpdateGrid();
				EMDataSet.POHeaderTblRow row = GetHeaderRow();
				for (int i=0;i<m_textBoxes.Length;i++)
				{
					SetText(m_textBoxes[i],row[m_fieldNames[i]].ToString());
				}
				for (int i=0;i<m_dateBoxes.Length;i++)
				{
					object field = row[m_dateFieldNames[i]];
					if (field is DateTime)
					{
						m_dateBoxes[i].Text = HelperFunctions.ToDateText((DateTime)field);
					}
					else
						m_dateBoxes[i].Text = "";
				}

				UpdateCurrencyControls();
				UpdateLocationDataSets();
				UpdateEnabled();
			}
		}

		
		void UpdateGrid()
		{
			using (new StopComboBoxUpdates(this))
			{
				// Disable remove, moveup, and movedown for the ack grid
				int selectedIndex = m_tabControl.SelectedIndex;
				bool enableDelete = (selectedIndex == 0);
				bool isEmptyTable = base.IsEmptyTable();
				enableDelete = enableDelete & !isEmptyTable;
				moveUpBtn.Enabled = enableDelete;
				moveDownBtn.Enabled = enableDelete;
				removeItemBtn.Enabled = enableDelete;

				// Not sure why I'm updating the UM combo when trying to update
				// the grid.
				bool isKg = DataInterface.IsMetric(m_emDataSet.POItemTbl);
				if (isKg)
				{
					umCombo.Text = "kg";
				}
				else
				{
					umCombo.Text = "lbs";			
				}
				this.FixUMButNoUpdates(umCombo.Text);

				EMDataSet.POItemTblDataTable itemTable = m_emDataSet.POItemTbl;
				if (selectedIndex == 0) // is PO grid
				{
					HelperFunctions.UpdateGrid(
						HelperFunctions.GetViewFrom(itemTable),
						poGrid,this,isKg,IsNewAllowed.Yes,IsReadOnly.No,
						HelperFunctions.GeneralFields);
				}
				else
				{
					itemTable = (EMDataSet.POItemTblDataTable)itemTable.Copy();
					// Remove message lines
					ArrayList listOfItemsToDelete = new ArrayList();
					foreach (EMDataSet.POItemTblRow row in itemTable.Rows)
					{
						if (!DataInterface.IsRowAlive(row))
							continue;
						if (row.IsItemNameNull() ||
							row.ItemName == "")
						{
							listOfItemsToDelete.Add(row);
						}
					}
					foreach(DataRow row in listOfItemsToDelete)
					{
						row.Delete();
					}
					HelperFunctions.UpdateGrid(
						HelperFunctions.GetViewFrom(itemTable),
						m_acknowledgementGrid,this,isKg,IsNewAllowed.No,
						IsReadOnly.No,
						HelperFunctions.AckFields);
				}
			}
		}
		
