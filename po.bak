				companyAdapter.Fill(locationDataSet.Tables["CompanyTbl"]);
				countryAdapter.Fill(locationDataSet.Tables["CountryTbl"]);
				locationAdapter.Fill(locationDataSet.Tables["LocationTbl"]);
				contactsAdapter.Fill(locationDataSet.Tables["ContactsTbl"]);
				DataInterface.UpdateComboBox(locationDataSet.Tables["LocationTbl"].DefaultView,"LocID","LocName",
											 locationCombo,null);
				contactsCombo.Items.Clear();
				foreach (EMDataSet.ContactsTblRow row in locationDataSet.ContactsTbl)
				{
					string fullname = row.FirstName + " " + row.LastName;
					DataInterface.TaggedItem t = new DataInterface.TaggedItem(row.ContactID,fullname);
					contactsCombo.Items.Add(t);
				}
