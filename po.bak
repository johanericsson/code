				box = millCombo;
			else
				box = customerCombo;
			
			box.Text = dlg.GetCompanyName();
		}
		private void addNewLocationBtn_Click(object sender, System.EventArgs e)
		{
			try
			{
				ComboBox box;
				if (sender == addNewMillLocationBtn)
				{
					box = vendLocationCombo;
				}
				else
				{
					box = shipToLocationCombo;
				}
			
				string company;
				if (sender == addNewMillLocationBtn)
					company = millCombo.Text;
				else
					company = customerCombo.Text;
				if (company == "")
					throw new Exception("No company was selected. Company must be "
						+   "selected before a location can be added");
				int compID = DataInterface.GetCompID(m_emDataSet,company);
				AddNewLocation dlg = new AddNewLocation(compID);
				DialogResult res = dlg.ShowDialog();
				if (res != DialogResult.OK)
					return;
				using (new StopComboBoxUpdates(this))
				{
					UpdateLocationDataSets(box);
				}
				DataInterface.SelectComboBox(box,dlg.m_locationID);
			}
			catch(Exception ex)
			{
				MessageBox.Show(ex.Message);
				return;
			}

		}



