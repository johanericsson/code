		public void MoveToNewCell(DataGrid grid)
		{
			DataGridCell cell = grid.CurrentCell;
			int row = cell.RowNumber;
			int column = cell.ColumnNumber;
			if (column == 0)
				column =1;
			else
				column = 0;
			grid.CurrentCell = new DataGridCell(row,column);
		}

		public void OnGridItemClicked(int selectedRow)
		{
			try
			{
				FromControls();
				EMDataSet.POHeaderTblRow headerRow = GetHeaderRow();
				if (headerRow.IsShipToNameNull() ||
					headerRow.ShipToName == "")
				{
					throw new Exception("Customer must be chosen before you can use the item selection window");
				}


				EMDataSet.POItemTblRow rowOut = HelperFunctions.GetItemRowOrCreate(m_emDataSet.POItemTbl,
					headerRow.POID,selectedRow);

				if (!rowOut.IsAcknowledgeDateNull())
				{
					MessageBox.Show("Item can not be changed since it has already been acknowledged.\n" +
						"Remove acknowledgement date first.","Can't change acknowledged item",
						MessageBoxButtons.OK,MessageBoxIcon.Stop);
					return;
				}

				string company = headerRow.ShipToName;
				EMDataSet.ItemTblRow rowIn = ChooseItem.CreateNewRow(company);
				if (rowIn == null)
					return;
			

				foreach (DataColumn column in rowOut.Table.Columns)
				{
					string columnName = column.ColumnName;
					if (columnName == "POID")
						continue;
					if (columnName == "POItemNumber")
						continue;
					if (columnName == "SeqNumber")
						continue;
					rowOut[column] = DBNull.Value;
				}
				
				if (!rowIn.IsItemNameNull())
					rowOut.ItemName = rowIn.ItemName;
	
				if (!rowIn.IsItemDescNull())
				{
					rowOut.ItemDesc = rowIn.ItemDesc;
				}
				UpdateGrid();
			}
			catch (Exception e)
			{
				MessageBox.Show(e.Message);
			}

		}

		public override bool IsValid()
		{
			EMDataSet.POHeaderTblRow row = GetHeaderRow();
			bool doesItemNameExist = false;
			foreach (EMDataSet.POItemTblRow childRow in m_emDataSet.POItemTbl.Rows)
			{
				if (!DataInterface.IsRowAlive(childRow))
				{
					continue;
				}
				if (!childRow.IsItemNameNull())
					if (childRow.ItemName != "")
						doesItemNameExist = true;
			}
			if (!doesItemNameExist)
				return true;
			string[] requiredFields = 
			{"PODate","PONumber","Terms","ShipCode","FOB","VendName","VendLocationName",
			"ShipToName","ShipToLocationName","ShipToContact"};
			string[] friendlyTitles = 
				{	"Purchase order date","PONumber","Terms","Ship Via","FOB","Mill Name","Mill Location Name",
					"Customer Name","Customer Location Name","Customer Contact"};
			ArrayList listOfFailedCriteria = new ArrayList();
			for (int i=0;i<requiredFields.Length;i++)
			{
				string fieldName = requiredFields[i];
				if (row.IsNull(fieldName))
				{
					listOfFailedCriteria.Add(i);
					continue;
				}
				if (row.Table.Columns[fieldName].DataType == typeof(string))
				{
					if ((string)row[fieldName] == "")
					{
						listOfFailedCriteria.Add(i);
						continue;
					}
				}
			}
			if (listOfFailedCriteria.Count == 0)
				return true;
			string errorMessage = "The following fields must be filled in before saving:";
			foreach (int failure in listOfFailedCriteria)
			{
				errorMessage += "\n" + friendlyTitles[failure];
			}
			MessageBox.Show(errorMessage,"Unable to save purchase order");
			return false;
		}

		public void NewGridTotal(decimal total)
		{
			EMDataSet.POHeaderTblRow row = GetHeaderRow();
			string otherTotal = total.ToString();
			row.OtherTotal = total;
			if (row.IsExchangeRateNull())
			{
				row.USTotal = row.OtherTotal;
				m_totalCostUSEdit.Text = otherTotal;
			}
			else
			{
				row.USTotal = row.OtherTotal * row.ExchangeRate;
			}
			UpdateCurrencyControls();
		}


